{
    "name": "前端应用",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf ./dist/ && mkdir dist",
        "start": "parcel src/index.html",
        "build": "parcel build --public-url . src/index.html --log-level 4",
        "debug": "npm run clean && npm run build && npm run start",
        "release": "npm run clean && rm -rf bin && mkdir bin && parcel build --public-url . src/index.html --no-source-maps && cp -rf dist/*.* bin"
    },
    "devDependencies": {
        "@babel/core": "^7.6.3",
        "parcel": "^1.12.4",
        "parcel-bundler": "^1.12.4"
    },
    "dependencies": {
        "@material-ui/core": "^4.5.0",
        "@material-ui/icons": "^4.4.3",
        "@material-ui/lab": "^4.0.0-alpha.28",
        "@types/react": "^16.9.5",
        "@types/react-dom": "^16.9.1",
        "react": "^16.10.2",
        "react-dom": "^16.10.2"
    }
}
